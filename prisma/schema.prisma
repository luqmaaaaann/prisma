
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//digunakan untuk memudahkan komunikasi antara database prisma dgn pemanggilan function
model Note {
  id String @id @default(cuid())
  title String
  content String?
  createdAt DateTime @default(now())
}
